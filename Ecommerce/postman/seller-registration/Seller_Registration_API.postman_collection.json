		{
			"name": "Authentication",
			"item": [
				{
					"name": "Login as BUYER",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    ",
									"    // Lưu access token",
									"    if (response.data && response.data.access_token) {",
									"        pm.environment.set('buyer_access_token', response.data.access_token);",
									"        console.log('✅ Buyer token saved');",
									"    }",
									"    ",
									"    // Lưu user ID nếu có",
									"    if (response.data && response.data.user && response.data.user.id) {",
									"        pm.environment.set('buyer_user_id', response.data.user.id);",
									"    }",
									"}",
									"",
									"pm.test('Status code is 200', function() {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test('Access token exists', function() {",
									"    const response = pm.response.json();",
									"    pm.expect(response.data.access_token).to.exist;",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"{{buyer_username}}\",\n  \"password\": \"{{buyer_password}}\"\n}"
						},
